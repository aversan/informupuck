/* nl-info */
.nl-info
	font-family $font-family-base

	&__row
		display flex
		flex-direction row
		flex-wrap nowrap
		flex-shrink 0
		box-sizing border-box
		align-self stretch
		align-items center

		& > *
			-ms-flex-negative 0
			flex-shrink 0

	&__spacer
		-ms-flex-positive 1
		flex-grow 1

	.info-date
	.info-comment
	.info-smotr
	&__tags
		font-size 10px

	.info-date
		color #999

	.info-comment
	.info-smotr
		color #4d739f

	&__tags
		color #999

		a
			text-decoration none
			color #999

			&:focus
			&:hover
				color #4d4d4d

	&__icons
		.icon + .icon
			margin-left 3px

	.nl-element-actual:hover &
	.nl-element:hover &
		.info-date
			color #aebfd3

		.info-comment
		.info-smotr
			color #aebfd3

		&__tags
			color #aebfd3

			a
				color #aebfd3

				&:focus
				&:hover
					color #fff

/* nl-element-actual */
.nl-element-actual
	box-sizing border-box
	padding-left 20px
	padding-right 30px
	height auto
	padding-top 17px
	padding-bottom 16px
	position relative

	& + &:before
		position absolute
		content ''
		display block
		border-top 1px solid #e6e6e6
		top 0
		left 20px
		right 30px

	.nl-text-actual
	.nl-info
		margin 5px 0 0 0

		&:first-child
			margin-top 0

	.nl-text-actual
		padding 0
		height 68px

		img
			margin 3px 10px 0 0

		p
			max-height 68px
			display -webkit-box
			-webkit-box-orient vertical
			overflow hidden
			text-overflow ellipsis
			word-wrap break-word
			-webkit-line-clamp 4
			float none
			width auto
			font-family $font-family-base

	&:hover
		color #fff

		&:before
			display none

/* nl-element */
.nl-element
	@extend .nl-element-actual

	& + &:before
		@extend .nl-element-actual + .nl-element-actual:before

	.nl-text
		@extend .nl-element-actual .nl-text-actual

		p
			@extend .nl-element-actual .nl-text-actual p
			margin 0
			-webkit-line-clamp 3
			max-height 51px

	.nl-info
		@extend .nl-element-actual .nl-info

	.nl-text
		height 97px

	.nl-text-a
		display block
		max-width none
		font-size 14px
		font-weight bold
		max-height 36px
		display -webkit-box
		-webkit-box-orient vertical
		overflow hidden
		text-overflow ellipsis
		word-wrap break-word
		-webkit-line-clamp 2
		margin-bottom 10px


